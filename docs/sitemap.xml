---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:image="http://www.sitemaps.org/schemas/sitemap-image/1.1" xmlns:video="http://www.sitemaps.org/schemas/sitemap-video/1.1">
<url>
	<loc>http://dmayance.com</loc>
	<lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
  <changefreq>monthly</changefreq>
  <priority>1.0</priority>
</url>
<url>
	<loc>http://dmayance.com/work/</loc>
	<lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
  <changefreq>monthly</changefreq>
  <priority>0.9</priority>
</url>
<url>
	<loc>http://dmayance.com/about/</loc>
	<lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
  <changefreq>monthly</changefreq>
  <priority>0.9</priority>
</url>
<url>
	<loc>http://dmayance.com/code/</loc>
	<lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
  <changefreq>monthly</changefreq>
  <priority>0.7</priority>
</url>
<url>
	<loc>http://dmayance.com/events/</loc>
	<lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
  <changefreq>monthly</changefreq>
  <priority>0.8</priority>
</url>
  {% for post in site.posts %}
    {% unless post.hide %}
		  <url>
		    <loc>http://dmayance.com{{ post.url }}</loc>
		    {% if post.lastmod %}
		    <lastmod>{{ post.lastmod | date_to_xmlschema }}</lastmod>
		    {% else %}
		    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
		    {% endif %}
		    <priority>0.5</priority>
		  </url>
	  {% endunless %}
  {% endfor %}
</urlset>
